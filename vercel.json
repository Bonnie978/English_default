{
  "version": 2,
  "builds": [
    {
      "src": "vocabulary-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/vocabulary-api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/vocabulary-app/$1"
    }
  ]
} 